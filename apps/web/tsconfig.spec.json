{
  "extends": "@cgr/project-config/tsconfig-nextjs.json",
  "compilerOptions": {
    "paths": {
      "@web/*": ["./src/*"]
    }
  },
  "include": [
    "jest.config.ts",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.test.tsx",
    "**/*.spec.tsx",
    "**/*.test.js",
    "**/*.spec.js",
    "**/*.test.jsx",
    "**/*.spec.jsx",
    "**/*.d.ts"
  ],
  "exclude": ["node_modules"]
}
