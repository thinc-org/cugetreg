{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test": {},
    "test:e2e": {
      "outputs": ["playwright-report/**"]
    }
  }
}
