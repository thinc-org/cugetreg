{
  "uglifyOptions": {
    "ie8": false,
    "debug": false,
    "sourceMap": false,
    "warnings": false,
    "compress": {
      "unsafe_comps": true,
      "properties": true,
      "keep_fargs": false,
      "pure_getters": true,
      "collapse_vars": true,
      "unsafe": true,
      "sequences": true,
      "dead_code": true,
      "drop_debugger": true,
      "comparisons": true,
      "conditionals": true,
      "evaluate": true,
      "booleans": true,
      "loops": true,
      "unused": true,
      "hoist_funs": true,
      "if_return": true,
      "join_vars": true,
      "drop_console": false,
      "pure_funcs": ["classCallCheck", "invariant", "warning"]
    },

    "parallel": true,
    "output": {
      "comments": false
    }
  }
}
